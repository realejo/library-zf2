{
  "name" : "realejo/library-zf2",
  "description" : "Biblioteca com models comuns utilizados nos projetos ZF2.",
  "type" : "library",
  "homepage" : "http://realejo.com.br",
  "license" : "BSD-3-Clause",
  "keywords" : [ "framework", "zf2", "realejo" ],
  "require" : {
    "php" : "~5.6 || ~7.0 || ~7.1",
    "zendframework/zend-db" : "^2.8.2",
    "zendframework/zend-cache" : "^2.7.2",
    "zendframework/zend-json" : "^2.6 || ^3.0.0",
    "zendframework/zend-paginator" : "^2.7.0",
    "zendframework/zend-serializer" : "^2.8.0"
  },
  "require-dev" : {
    "squizlabs/php_codesniffer": "^2.8.1",
    "phpunit/PHPUnit" : "~5.7"
  },
  "autoload-dev" : {
    "psr-4" : {
      "RealejoTest\\" : "test/"
    }
  },
  "autoload" : {
    "psr-4" : {
      "Realejo\\" : "src/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
