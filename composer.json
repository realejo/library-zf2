{
    "name": "realejo/library-zf2",
    "description": "Biblioteca com models comuns utilizados nos projetos ZF2.",
    "type": "library",
    "homepage": "http://realejo.com.br",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "realejo"
    ],
    "require": {
        "php": "~7.1 || ~7.2 || ~7.3 || ~7.4",
        "laminas/laminas-db": "^2.8.2",
        "laminas/laminas-cache": "^2.7.2",
        "laminas/laminas-json": "^2.6 || ^3.0.0",
        "laminas/laminas-paginator": "^2.7.0",
        "laminas/laminas-serializer": "^2.8.0",
        "laminas/laminas-dependency-plugin": "^2.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8.1",
        "phpunit/phpunit": "^8.4 || ^7.5"
    },
    "autoload-dev": {
        "psr-4": {
            "RealejoTest\\": "test/"
        }
    },
    "autoload": {
        "psr-4": {
            "Realejo\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
